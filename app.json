{
  "expo": {
    "name": "echarts-touch-sample",
    "slug": "echarts-touch-sample",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Example project to demonstrate touch problem when using echarts on mobile",
    "githubUrl": "https://github.com/JonathanRufino/echarts-touch-sample"
  }
}